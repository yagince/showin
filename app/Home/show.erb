<div  id="timeline-detail" data-role="page" class="timeline-page" data-add-back-btn="true">
  <div data-role="header" data-position="fixed" data-tap-toggle="false">
    <h1><%= @timeline.user.name %></h1>
  </div>

  <div id="tweet-detail" data-role="content">
  </div>
  <script>
    $("#tweet-detail").html(timelineDetailHtml(<%= @timeline_json %>, "#twitter-timeline-detail-template"));
  </script>

  <%= render partial: "/footer" %>
</div>
